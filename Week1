name = "Mandy"
age = 22
message = "Hello, %s. You are %d years old." % (name, age)
print(message)
Name="Mandy"
Age=22
Message ="Hello,{}. You are {} years old.".format(Name,Age)
print(Message)   
MESSAGE=f"Good morning,{name}"
print(MESSAGE)
text="hello\nworld"
print(text)
